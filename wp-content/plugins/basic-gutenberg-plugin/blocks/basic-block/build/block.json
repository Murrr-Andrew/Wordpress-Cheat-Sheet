{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/basic-block",
  "version": "0.1.0",
  "title": "Basic Block",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "textdomain": "basic-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "message": {
      "type": "string",
      "source": "text",
      "selector": "div",
      "default": ""
    },
    "bg_color": {
      "type": "string",
      "default": "#000000"
    },
    "text_color": {
      "type": "string",
      "default": "#ffffff"
    }
  }
}